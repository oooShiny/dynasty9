{# Using iframe method to avoid CORS issues with muse.ai API #}
{% for item in items %}
  {% set muse_id = item.content['#context']['value'] %}

  <iframe src="https://muse.ai/embed/{{ muse_id }}"
          style="width: 100%; aspect-ratio: 16/9;"
          allowfullscreen
          allow="autoplay; fullscreen"
          loading="lazy"></iframe>
{% endfor %}
